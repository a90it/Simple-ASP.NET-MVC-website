@model MvcTemplate.Web.ViewModels.Jokes.JokesByCategoryViewModel
@{
    ViewBag.Title = "Category";
}

<h4>Joke category: @Model.Category.Name</h4>
<div class="row">
    @foreach (var joke in Model.Jokes)
    {
        <div class="col-md-4">
            <a href="@joke.Url">
                @Html.Raw(joke.Content.Replace("\n", "<br />"))
            </a>
        </div>
    }
</div>
